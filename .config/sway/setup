#!/bin/bash
echo '#' > ~/.config/sway/config
echo '#' >> ~/.config/sway/config
echo '#' >> ~/.config/sway/config
echo '# AUTOGENERATED FILE' >> ~/.config/sway/config
echo '#' >> ~/.config/sway/config
echo '# change ~/.config/sway/base' >> ~/.config/sway/config
echo '# put system specific stuff in ~/.config/sway/local' >> ~/.config/sway/config
echo '# changes in here will not persist' >> ~/.config/sway/config
echo '#' >> ~/.config/sway/config
echo '#' >> ~/.config/sway/config
echo '#' >> ~/.config/sway/config
cat ~/.config/sway/base >> ~/.config/sway/config
cat ~/.config/sway/local >> ~/.config/sway/config
